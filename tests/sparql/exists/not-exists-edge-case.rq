PREFIX ex: <http://example.org/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>

# This should return NO results - alice has complete profile
SELECT ?person WHERE {
    ?person foaf:name "Alice Smith" .
    FILTER NOT EXISTS { 
        ?person foaf:name ?name .
        ?person foaf:age ?age .
        ?person foaf:email ?email 
    }
}