set -e

# Source the test environment
. "${srcdir:-.}/test-common.sh"

# Test description
echo "1..1"
echo "# Test 3: non-NULL vs NULL (should be different)"

# Run the comparison - should fail (different)
if "$RASQAL_COMPARE" -R xml -e "$srcdir/null-vs-value.xml" -a "$srcdir/null-both.xml" >/dev/null 2>&1; then
